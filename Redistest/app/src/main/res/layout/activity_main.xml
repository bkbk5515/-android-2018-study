<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/sexbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="14dp"
        android:text="Sex Set"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="52dp" />

    <Button
        android:id="@+id/agebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/sexbtn"
        android:layout_marginStart="123dp"
        android:text="Age Set"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="146dp" />

    <Button
        android:id="@+id/ageget"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/sexbtn"
        android:layout_marginBottom="31dp"
        android:text="Age Get" />

    <Button
        android:id="@+id/sexget"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/agebtn"
        android:layout_alignTop="@+id/ageget"
        android:text="Sex Get" />

    <Button
        android:id="@+id/livestartbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/sexbtn"
        android:layout_marginTop="108dp"
        android:text="livestart" />

    <Button
        android:id="@+id/vodclickbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/livestartbtn"
        android:layout_marginStart="136dp"
        android:text="vodclick" />

    <Button
        android:id="@+id/lankgetbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/livestartbtn"
        android:layout_marginEnd="46dp"
        android:text="vodlank" />

</RelativeLayout>